    "del": "^2.0.2",
    "eslint": "^1.7.3",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-react": "^3.6.3",
    "jest": "^0.1.40",
    "jest-cli": "^0.6.1",
    "react-addons-create-fragment": "^0.14.1",
